  "compilerOptions": {
      "module": "commonjs",
      "moduleResolution": "node",
      "esModuleInterop": true,
      "target": "es5",
      "sourceMap": true,
      "strict": true,
      "noImplicitAny": true,
      "resolveJsonModule": true,
      "lib": ["ESNext"]
  },
  "exclude": ["node_modules"]
}
